	<include name="TVGuideItemLayout">
	    <control type="group">
	        <animation effect="slide" end="-9" time="0" condition="substring(System.Time,PM) | substring(System.Time,AM)">Conditional</animation>
    	    <control type="image">
    	    	<description>Clock</description>
    	    	<posx>18</posx>
    	    	<posy>78</posy>
    	    	<width>31</width>
    	    	<height>31</height>
    	    	<aspectratio>keep</aspectratio>
    	    	<texture>windows/tvguide/clock.png</texture>
    	    </control>
    		<control type="label">
    			<description>Airing time</description>
    			<posx>54</posx>
    			<posy>80</posy>
    			<width>120</width>
    			<height>20</height>
    			<label>$INFO[ListItem.Property(ShortTime)]</label>
    			<textcolor>FF787878</textcolor>
    			<font>Font-Condensed-S34</font>
    			<align>left</align>
    		</control>
    	</control>
		<control type="image">
			<description>Thumb</description>
			<posx>12</posx>
			<posy>8</posy>
			<width>151</width>
			<height>59</height>
			<aspectratio>keep</aspectratio>
			<texture background="true">$INFO[ListItem.Thumb]</texture>
			<visible>StringCompare(Window(home).Property(TVGuide.ThumbType),0)</visible>
		</control>
		<control type="image">
			<description>Banner</description>
			<posx>12</posx>
			<posy>8</posy>
			<width>151</width>
			<height>59</height>
			<aspectratio>keep</aspectratio>
			<texture background="true">$INFO[ListItem.Property(Path),,banner.jpg]</texture>
			<visible>StringCompare(Window(home).Property(TVGuide.ThumbType),1)</visible>
		</control>
		<control type="image">
			<description>Logo</description>
			<posx>12</posx>
			<posy>8</posy>
			<width>151</width>
			<height>59</height>
			<aspectratio>keep</aspectratio>
			<texture background="true">$INFO[ListItem.Property(Path),,logo.png]</texture>
			<visible>StringCompare(Window(home).Property(TVGuide.ThumbType),2)</visible>
		</control>
	</include>
	
